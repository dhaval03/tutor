											<input type="text" name="fax" value="{{ fax }}" placeholder="{{ entry_fax }}" id="input-fax" class="form-control" />
											<div id="error-fax" class="invalid-feedback"></div>
											
											</div>
										</div>
									</div>
								</div> 
								
								<div class="row"> 
