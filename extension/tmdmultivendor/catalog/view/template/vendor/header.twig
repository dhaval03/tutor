<!DOCTYPE html>
<html dir="{{ direction }}" lang="{{ lang }}">
	<head>
		<meta charset="UTF-8" />
		<title>{{ title }}</title>
		<base href="{{ base }}" />
		{% if description %}
		<meta name="description" content="{{ description }}" />
		{% endif %}
		{% if keywords %}
		<meta name="keywords" content="{{ keywords }}" />
		{% endif %}
		<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
		
		  <link href="extension/tmdmultivendor/catalog/view/stylesheet/bootstrap.css" rel="stylesheet" media="screen"/>
		  <link href="catalog/view/stylesheet/fonts/fontawesome/css/all.min.css" type="text/css" rel="stylesheet"/>
			<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css">
		  <script src="extension/tmdmultivendor/catalog/view/javascript/vendor/jquery-3.6.0.min.js" type="text/javascript"></script>
		  <script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/datetimepicker/moment.min.js"></script>
		  <script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/datetimepicker/moment-with-locales.min.js"></script>
		  <script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/datetimepicker/daterangepicker.js"></script>
		  <link href="extension/tmdmultivendor/catalog/view/javascript/vendor/datetimepicker/daterangepicker.css" rel="stylesheet" type="text/css"/>
		  <script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/common.js"></script>

		<link type="text/css" href="extension/tmdmultivendor/catalog/view/stylesheet/vendor/vendordashboard.css" rel="stylesheet" media="screen" />
		<script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/ckeditor/ckeditor.js"></script>
		<script type="text/javascript" src="extension/tmdmultivendor/catalog/view/javascript/vendor/ckeditor/adapters/jquery.js"></script>
		
		
		{% for style in styles %}
		<link type="text/css" href="{{ style.href }}" rel="{{ style.rel }}" media="{{ style.media }}" />
		{% endfor %}
		{% for link in links %}
		<link href="{{ link.href }}" rel="{{ link.rel }}" />
		{% endfor %}
		<script src="extension/tmdmultivendor/catalog/view/javascript/vendor/common.js" type="text/javascript"></script>
		{% if lang=='ar' %}
		 <link href="extension/tmdmultivendor/catalog/view/stylesheet/vendor/bootstrap-rtl.css" rel="stylesheet">
		 <link href="extension/tmdmultivendor/catalog/view/stylesheet/vendor/bootstrap-rtl.min.css" rel="stylesheet">
		 <link href="extension/tmdmultivendor/catalog/view/stylesheet/vendor/rtl.css" rel="stylesheet">
		 <style>
		#column-left + #content {padding-right: 50px;}
		</style>
		{% endif %}	
	</head>
<body>
<div id="alert" class="position-fixed top-0 end-0 p-3" style="z-index: 9999;"></div>
<div id="container">
<header id="header" class="navbar navbar-static-top">
  <div class="navbar-header">
    {% if logged %}
    <a type="button" id="button-menu" class="pull-left"><i class="fas fa-indent"></i></a>
    {% endif %}
    
  </div>
  {% if logged %}
  <ul class="nav pull-right rightmenu">
    <li><a href="{{ myprofile }}" ><span class="hidden-xs hidden-sm hidden-md">{{ text_myprofile }} </span> <i class="fa fa-user"></i></a></li>
    {% if customer2vendor==1 %}
	<li><a href="{{ allseller }}"><span class="hidden-xs hidden-sm hidden-md">{{ text_allsellers }} </span> <i class="fa fa-users"></i></a></li>
	 {% endif %}
    
	
	
	<li><a href="{{ logout }}"><span class="hidden-xs hidden-sm hidden-md">{{ text_logout }}</span> <i class="fa fa-sign-out fa-lg"></i></a></li>
	 {% if seller_logged %}
    <li><a href="{{ home }}" ><img src="{{ logos }}" alt="{{ name }}" title="{{ name }}" class="storelogo"/></a>
    {% endif %}
	
	</li>
  </ul>
  {% endif %}
</header>