							<span class="fas fa-stack"><i class="far fa-star fa-stack-2x"></i></span>
						  {% else %}
							<span class="fas fa-stack"><i class="fas fa-star fa-stack-2x"></i><i class="far fa-star fa-stack-2x"></i></span>
						  {% endif %}
						{% endfor %}
					</div>
					<!--03 10 2019 -->

					{% if vendorcontact !=1 %}
					<ul class="list-inline">
						{% if vendor_hidevponeno %}
							<li class="nav-item list-inline-item"><i class="fa fa-phone-alt"></i> {{ telephone }}</li>
						{% endif %}

						{% if vendor_hidevemail %}
							<li class="nav-item list-inline-item"><i class="fas fa-envelope"></i> <a class="vendoremail" href="mailto:{{ email }}" target="_top">{{ email }}</a></li>
						{% endif %}
					</ul>


					{% if vendor_hidevsocialicon %}
					<ul class="list-unstyled btn-social">
						<li>
							<!--Social icon-->
								{% if facebookurl %}
								<a href="{{ facebookurl }}" target="new"><i class="fab fa-facebook"></i></a>
								{% endif %}
								{% if googleurl %}
								<a href="{{ googleurl }}" target="new"><i class="fab fa-google"></i></a>
								{% endif %}
								
								{% if whatsapp_url %}
								<a href="{{ whatsapp_url }}" target="new"><i class="fab fa-whatsapp" ></i></a>
								{% endif %}
								
								{% if instagram_url %}
								<a href="{{ instagram_url }}" target="new"><i class="fab fa-instagram" ></i></a>
								{% endif %}
								
								{% if twitter_url %}
								<a href="{{ twitter_url }}" target="new"><i class="fab fa-twitter" ></i></a>
								{% endif %}
								
								{% if snapchat_url %}
								<a href="{{ snapchat_url }}" target="new"><i class="fab fa-snapchat-ghost" ></i></a>
								{% endif %}
								
								{% if pinterest_url %}
								<a href="{{ pinterest_url }}" target="new"><i class="fab fa-pinterest" ></i></a>
								{% endif %}
								
								{% if youtube_url %}
								<a href="{{ youtube_url }}" target="new"><i class="fab fa-youtube" ></i></a>
								{% endif %}
								
								{% if linkidin_url %}
								<a href="{{ linkedin_url }}" target="new"><i class="fab fa-linkedin-in" ></i></a>
								{% endif %}
								
								{% if tiktok_url %}
								<a href="{{ tiktok_url }}" target="new"><i class="fas fa-music" ></i></a>
								{% endif %}								
							<!--Social icon-->
							{% if vendorfindme %}
							<a href="{{ vendorfindme }}" target="new"><i class="fas fa-map-marker-alt" ></i></a>
							{% endif %}
						</li>
					</ul>
					{% endif %}
					{% endif %}
				</div>
				<div class="onhover">
					<img src="{{ logos }}" class="img-fluid" title="logo" alt="logo" />
				</div>
			</div>
