											<label class="col-form-label col-sm-12" for="input-postcode">{{ entry_postcode }}</label>
											<div class="col-sm-12">
											<input type="text" name="postcode" value="{{ postcode }}" placeholder="{{ entry_postcode }}" id="input-postcode" class="form-control" />
											{% if chkpostcode==1 %}
												<div id="error-postcode" class="invalid-feedback"></div>
											{% endif %}
											</div>
										</div>
									</div>
									
									<div class="col-sm-4 {% if not status_country %} d-none {% endif %}">
										<div class="row mb-3 {% if required_country %} required {% endif %}">
											<label class="col-form-label col-sm-12" for="input-country">{{ entry_country }}</label>
											<div class="col-sm-12">
												<select name="country_id" id="input-country" class="form-control">
													<option value="">{{ text_select }}</option>
													{% for country in countries %}
													{% if country.country_id == country_id %}
													<option value="{{ country.country_id }}" selected="selected">{{ country.name }}</option>
													{% else %}
													<option value="{{ country.country_id }}">{{ country.name }}</option>
													{% endif %}
													{% endfor %}
												</select>
												<div id="error-country" class="invalid-feedback"></div>
											</div>
										</div>
									</div>
								</div>
								
								<div class="row"> 
									<div class="col-sm-4 {% if not status_zone %} d-none {% endif %}">
										<div class="row mb-3 {% if required_zone %} required {% endif %}">
											<label class="col-form-label col-sm-12" for="input-zone">{{ entry_zone }}</label>
											<div class="col-sm-12">
												<select name="zone_id" id="input-zone" class="form-control">
												</select>
												<div id="error-zone" class="invalid-feedback"></div>
											</div>
										</div>
									</div>
								</div>
								
								<div class="row">
									<div class="col-sm-8 {% if not status_about %} d-none {% endif %}">
										<div class="row mb-3 {% if required_about %} required {% endif %}">
											<label class="col-form-label col-sm-12" for="input-about">{{ entry_about }}</label>
											<div class="col-sm-12">
												<textarea name="about" rows="5" placeholder="{{ entry_about }}" id="input-about" class="form-control" >{{ about }}</textarea>
												
												<div id="error-about" class="invalid-feedback"></div>
											</div>
										</div>
									</div>

									<div class="col-sm-4 {% if not status_image %} d-none {% endif %}">
										<div class="row mb-3">
											<label class="col-form-label col-sm-12" for="input-logo">{{ entry_image }}</label>
											<div class="col-sm-12">
												<div class="imagebox">
													<span id="thumb-image"><img src="{{ thumb }}" alt="" title="" class="thumbnail"/></span>
												</div>
												<br/>
												<button type="button" id="button-upload1" data-loading-text="{{ text_loading }}" 
												class="btn btn-default"><i class="fa fa-upload"></i> {{ button_upload }}</button>
												<input type="hidden" name="image" value="{{ image }}" id="input-image" />
											</div>
										</div>
									</div>
								</div>
							</div>
							
							<div class="tab-pane" id="tab-generalstore">
								<ul class="nav nav-tabs" id="language">
									{% for language in languages %}
									<li><a href="#language{{ language.language_id }}" data-bs-toggle="tab" class="nav-link{% if loop.first %} active{% endif %}"><img src="catalog/language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /> {{ language.name }}</a></li>
									{% endfor %}
								</ul>
								<div class="tab-content">
